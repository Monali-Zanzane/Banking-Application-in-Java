drop table Login_Table;
 

CREATE TABLE Login_Table (
   
    userName  VARCHAR(20) NOT NULL,
    passwords VARCHAR(20),
    question VARCHAR(30),
    answer VARCHAR(20),
    CONSTRAINT Login_Table_userName_UQ UNIQUE(userName)
    
    );
